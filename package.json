{
	"name": "template-11ty",
	"version": "0.1.0",
	"description": "静的webサイトの制作用テンプレート",
	"main": "index.js",
	"author": "Yusaku SATO",
	"license": "MIT",
	"scripts": {
		"dev": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --incremental --quiet",
		"build": "npm run build:eleventy",
		"build:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --quiet",
		"co": "npm run commit",
		"commit": "cz"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@11ty/eleventy": "^2.0.1",
		"commitizen": "^4.3.0",
		"cssnano": "^7.0.5",
		"cz-conventional-changelog": "^3.3.0",
		"eleventy-plugin-postcss": "^1.0.4",
		"eleventy-plugin-static-jsx": "^1.0.8",
		"postcss": "^8.4.41",
		"postcss-import": "^16.1.0",
		"static-jsx": "^1.0.1"
	},
	"volta": {
		"node": "22.7.0"
	},
	"dependencies": {
		"normalize.css": "^8.0.1"
	}
}
